<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="petStore"
          class="com.dantefung.service.v1.PetStoreService" >
    </bean>
    <bean id="beanInitializeLogger"
          class="com.dantefung.sample.postprocessor.BeanInitializeLogger" >
    </bean>
    <!--BeanNameAware test-->
    <bean id="userService"
          class="com.dantefung.sample.aware.UserService" >
    </bean>
    <!--BeanFacotryAware test-->
    <bean id="goodService"
          class="com.dantefung.sample.aware.GoodService" >
    </bean>
    <!--BeanClassLoaderAware test-->
    <bean id="orderService"
          class="com.dantefung.sample.aware.OrderService" >
    </bean>
</beans>